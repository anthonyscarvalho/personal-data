    "description": "",
    "author": "Anthony Carvalho",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build && cp ./.env ./dist/.env && ln -s ../MediaCollection ./dist/MediaCollection",
        "publish": "nest build && cp ./.env ./dist/.env",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "dev": "nest start --watch",
        "debug": "nest start --debug --watch",
        "start": "nest start",
        "start:prod": "node dist/server-ts/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:dev": "NODE_ENV=development jest --forceExit --detectOpenHandles"
    },
    "dependencies": {
        "@nestjs/bull": "^10.2.1",
